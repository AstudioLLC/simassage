{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "arrilot/laravel-widgets": "^3.13",
        "astrotomic/laravel-translatable": "^11.9",
        "biscolab/laravel-recaptcha": "^6.0",
        "ckfinder/ckfinder-laravel-package": "v3.5.1.6",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "k3rnel/omnipay-arca": "^0.5.0",
        "laravel/fortify": "^1.6",
        "laravel/framework": "^8.0",
        "laravel/jetstream": "^1.3",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "league/omnipay": "^3.2",
        "livewire/livewire": "^2.0",
        "mcamara/laravel-localization": "^1.6",
        "orangehill/iseed": "^2.6",
        "ptuchik/omnipay-idram": "^1.0",
        "spatie/laravel-medialibrary": "^8.0.0",
        "spatie/laravel-translatable": "^4.5",
        "unisharp/laravel-filemanager": "^2.2",
        "yajra/laravel-datatables": "^1.5",
        "zakhayko/settings": "^1.1",
        "zakhayko/translator": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
